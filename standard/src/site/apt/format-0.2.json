{
  "Record": {
    "type": "union",
    "types": ["RecordPointer", "CatalogFile", "CompleteFile", "CatalogFolder", "CompleteFolder"]
  },
  "RecordPointer": {
    "type": "record",
    "fields": {
      "volumeNumber": "int",
      "blockOffset": "int",
      "recordOffset": "int"
    }
  },
  "CatalogFile": {
    "type": "record",
    "fields": {
      "pointer": "RecordPointer",
      "header": "RecordHeader",
      "size": "int"
    }
  },
  "CompleteFile": {
    "type": "record",
    "fields": {
      "header": "RecordHeader",
      "content": "binary"
    }
  },
  "CatalogFolder": {
    "type": "record",
    "fields": {
      "pointer": "RecordPointer",
      "header": "RecordHeader"
    }
  },
  "CompleteFolder": {
    "type": "record",
    "fields": {
      "header": "RecordHeader"
    }
  },
  "RecordHeader": {
    "type": "record",
    "fields": {
      "id": "int",
      "parentId": "int",
      "name": "text",
      "attributes": "Attributes"
    }
  },
  "Attributes": {
    "type": "multimap",
    "items": {
      "lastModifiedTime": "int"
    }
  },
  "Block": {
    "type": "union",
    "types": ["reserved", "PlainBlock", "LzmaBlock", "AesBlock"]
  },
  "PlainBlock": {
    "type": "record",
    "fields": {
      "chunk": "binary",
      "checksumOfChunk": "Adler32Checksum"
    }
  },
  "LzmaBlock": {
    "type": "record",
    "fields": {
      "partOfCompressedChunk": "binary",
      "checksumOfPart": "Adler32Checksum"
    }
  },
  "AesBlock": {
    "type": "record",
    "fields": {
      "encryptedBlock": "binary"
    }
  },
  "Adler32Checksum": {
    "type": "fixed",
    "size": "4"
  }
}
